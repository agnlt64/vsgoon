{
  "name": "vsgoon",
  "displayName": "vsgoon",
  "description": "goon corner for vscode",
  "icon": "images/icon.jpg",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "agnlt64",
  "repository": {
    "type": "github",
    "url": "https://github.com/agnlt64/vsgoon"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "goonView",
          "name": "Goon Corner",
          "type": "webview",
          "icon": "media/icon.svg"
        }
      ]
    },
    "configuration": {
      "title": "Goon Corner",
      "properties": {
        "vsgoon.provider": {
          "type": "string",
          "enum": [
            "waifu.pics",
            "waifu.im"
          ],
          "enumDescriptions": [
            "waifu.pics - gifs and images, lots of SFW content, hardcore NSFW. using all available categories.",
            "waifu.im - only images, high res, lots of NSFW and ecchi content. using categories: maid (sfw), oppai (sfw), ass (nsfw), ecchi (nsfw)."
          ],
          "default": "waifu.pics",
          "description": "goon images provider"
        },
        "vsgoon.waifuPicsCategories": {
          "type": "object",
          "title": "categories",
          "properties": {
            "sfw": {
              "type": "array",
              "default": [
                "waifu",
                "neko",
                "shinobu",
                "megumin",
                "bully",
                "cuddle",
                "cry",
                "hug",
                "awoo",
                "kiss",
                "lick",
                "pat",
                "smug",
                "bonk",
                "yeet",
                "blush",
                "smile",
                "wave",
                "highfive",
                "handhold",
                "nom",
                "bite",
                "glomp",
                "slap",
                "kill",
                "kick",
                "happy",
                "wink",
                "poke",
                "dance",
                "cringe"
              ]
            },
            "nsfw": {
              "type": "array",
              "default": [
                "waifu",
                "neko",
                "trap",
                "blowjob"
              ]
            }
          }
        },
        "vsgoon.waifuImCategories": {
          "type": "object",
          "title": "categories",
          "properties": {
            "sfw": {
              "type": "array",
              "default": [
                "maid",
                "oppai"
              ]
            },
            "nsfw": {
              "type": "array",
              "default": [
                "ass",
                "ecchi"
              ]
            }
          }
        },
        "vsgoon.allowNSFW": {
          "type": "boolean",
          "default": false,
          "description": "Allow NSFW images in the goon corner"
        },
        "vsgoon.autoRefresh": {
          "type": "boolean",
          "default": true,
          "description": "Displays a new image after a delay"
        },
        "vsgoon.refreshDelay": {
          "type": "number",
          "default": 5,
          "description": "Number of seconds between each new image. Not used if autoRefresh is false."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.46.3",
    "eslint": "^9.39.1",
    "esbuild": "^0.27.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}